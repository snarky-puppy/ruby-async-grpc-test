        <JSR223Listener guiclass="TestBeanGUI" testclass="JSR223Listener" testname="Calculate successful Throughput">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">// Retrieve start time and success count
long testStartTime = props.get(&quot;testStartTime&quot;).toLong()
long testEndTime = System.currentTimeMillis()
long durationSeconds = (testEndTime - testStartTime) / 1000

long successCount = props.get(&quot;successCount&quot;).toLong()

// Calculate throughput
double throughput = durationSeconds &gt; 0 ? (double) successCount / durationSeconds : 0

// Log the throughput
log.info(&quot;===== Successful Throughput =====&quot;)
log.info(&quot;Total Successful Requests: &quot; + successCount)
log.info(&quot;Test Duration (seconds): &quot; + durationSeconds)
log.info(&quot;Throughput (requests/second): &quot; + throughput)
</stringProp>
        </JSR223Listener>
