          <stringProp name="parameters"></stringProp>
          <stringProp name="script">// Initialize counters and start time on the first sample
if (props.get(&quot;successCount&quot;) == null) {
    props.put(&quot;successCount&quot;, &quot;0&quot;)
}

if (props.get(&quot;testStartTime&quot;) == null) {
    props.put(&quot;testStartTime&quot;, System.currentTimeMillis().toString())
}

// Increment success count if sample is successful
if (prev.isSuccessful()) {
    long successCount = props.get(&quot;successCount&quot;).toLong()
    successCount += 1
    props.put(&quot;successCount&quot;, successCount.toString())
}
</stringProp>
